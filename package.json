{
  "name": "proshop",
  "version": "1.0.0",
  "description": "eCommerce app with MERN",
  "main": "server.ts",
  "type": "module",
  "scripts": {
    "start": "npx tsx api/server.ts",
    "server": "nodemon -e ts  --exec \"npx tsx api/server.ts\"",
    "client": "npm start frontend --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.9.0",
    "concurrently": "^9.1.0",
    "dotenv": "^16.4.5",
    "nodemon": "^3.1.7",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "express": "^4.21.1",
    "mongodb": "^6.10.0"
  }
}
