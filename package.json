{
  "name": "express-to-render",
  "version": "1.0.0",
  "description": "eCommerce app with MERN",
  "main": "server.ts",
  "type": "module",
  "scripts": {
    "start": "npx tsx api/server.ts",
    "server": "nodemon -e ts  --exec \"npx tsx api/server.ts\"",
    "client": "npm start frontend --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "npx tsx api/seeder.ts",
    "data:destroy": "npx tsx api/seeder.ts -d",
    "build": "npm install && npm install --prefix frontend && npm run build --prefix frontend"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.0",
    "@types/node": "^22.9.0",
    "concurrently": "^9.1.0",
    "nodemon": "^3.1.7",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "mongodb": "^6.10.0",
    "mongoose": "^8.8.1"
  }
}
